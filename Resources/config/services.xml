<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

   <parameters>
        <parameter key="vellozzi_urlshortener.manager.urlshortener.class">Vellozzi\UrlShortenerBundle\Manager\UrlToTagManager</parameter>
    </parameters>
    <services>
        <service id="vellozzi_urlshortener.urlshortener_manager" class="%vellozzi_urlshortener.manager.urlshortener.class%" >
            <call method="setEm">
             <argument type="service" id="doctrine.orm.entity_manager" />
            </call>            
            <call method="setLogger">
             <argument type="service" id="monolog.logger" />
            </call>
        </service>
      
    </services>
</container>
